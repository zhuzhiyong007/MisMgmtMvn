<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="3.1"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
  <display-name>MisMgmtMvn</display-name>
  <welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
  </welcome-file-list>
  <jsp-config>
    <taglib>
      <taglib-uri>SparkMisTag</taglib-uri>
      <taglib-location>/WEB-INF/SparkMisTag.tld</taglib-location>
    </taglib>
  </jsp-config>
  <error-page>
    <error-code>404</error-code>
    <location>/error404.jsp</location>
  </error-page>
  <error-page>
    <exception-type>java.lang.Exception</exception-type>
    <location>/exception.jsp</location>
  </error-page>
  <error-page>
    <exception-type>java.lang.NullException</exception-type>
    <location>/error.jsp</location>
  </error-page>
  <listener>
    <listener-class>org.springframework.web.util.Log4jConfigListener</listener-class>
  </listener>
  <context-param>
    <param-name>log4jConfigLocation</param-name>
    <param-value>/log4j.properties</param-value>
  </context-param>
  <context-param>
    <param-name>log4jRefreshInterval</param-name>
    <param-value>60000</param-value>
  </context-param>
  <context-param>
    <param-name>userSessionKey</param-name>
    <param-value>USERSESSONKEY</param-value>
  </context-param>
  <context-param>
    <param-name>rediretPage</param-name>
    <param-value>/Login.jsp</param-value>
  </context-param>
  <context-param>
    <param-name>uncheckUrls</param-name>
    <param-value>/Login.jsp,/UserLogin</param-value>
  </context-param>
  <filter>
    <filter-name>encoding</filter-name>
    <filter-class>net.zx.lims.core.net.EncodingFilter</filter-class>
    <init-param>
      <param-name>charset</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>encoding</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <!-- <filter>
    <filter-name>LoginFilter</filter-name>
    <filter-class>net.zx.lims.core.net.LoginFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>LoginFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping> -->
  <servlet>
    <servlet-name>UserLogin</servlet-name>
    <servlet-class>net.zx.lims.core.net.UserLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserLogin</servlet-name>
    <url-pattern>/UserLogin</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FrontServlet</servlet-name>
    <servlet-class>net.zx.lims.core.net.FrontServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FrontServlet</servlet-name>
    <url-pattern>/FrontServlet</url-pattern>
  </servlet-mapping>
</web-app>